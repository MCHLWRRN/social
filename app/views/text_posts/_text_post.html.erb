<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">
			<%= text_post.title %>
		</h3>
		<%= link_to("#{time_ago_in_words text_post.created_at} ago"), post_path(image_post) %>
	</div>

	<div class="panel-body">
		<p><em>By <%= text_post.user.name %></em></p>

		<%= text_post.body %>
	</div>
</div>